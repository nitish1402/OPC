using namespace std;


int main()
{
     int n,i,j,k;
     int A[100003],B[100003];
     map<int,int> CA;
     map<int,int> CB;
     cin>>n;
     FOR(i,n)
     {
        cin>>A[i]>>B[i];
        CA[A[i]]++;
        CB[B[i]]++;
     }
     //sort(CA,CA+CA.size());
     //sort(CB,CB+CB.size());
     int l,m;
     //l=CA[MAX-1];
     //m=CB[MAX-1];
     cout<<l<<" "<<m<<endl;
